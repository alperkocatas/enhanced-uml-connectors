        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_3fLlsChjEe2Cds-4L53svA" name="LessFrequentRequesterConnectorBehavior" specification="_KoQvdShkEe2s85KTtru1_A">
          <language>Alf</language>
          <body>Integer returnValue = this.DefaultReply;

if (this.InPort[1].RequestBuffer.size() == this.MaxRequestCount)
{
    // Merge requests:
    Integer sum = 0;
    while (this.InPort[1].RequestBuffer.size() != 0)
    {
        Integer request = this.InPort[1].RequestBuffer.removeFirst();
        if (request != null)
        {
            sum = sum + request;
        }
    }
    
    returnValue = sum;
}

return returnValue;</body>
        </ownedBehavior>
