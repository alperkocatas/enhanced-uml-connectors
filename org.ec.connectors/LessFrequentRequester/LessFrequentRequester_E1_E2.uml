        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_3fLlsChjEe2Cds-4L53svA" name="LessFrequentRequesterConnectorBehavior" specification="_IPzRNkq8Ee2pJpQkqkRU5w">
          <language>Alf</language>
          <body>Integer returnValue = this.DefaultReply;&#xD;
&#xD;
if (this.InPort[1].RequestBuffer.size() == this.MaxRequestCount)&#xD;
{&#xD;
    // Merge requests:&#xD;
    Integer sum = 0;&#xD;
    while (this.InPort[1].RequestBuffer.size() != 0)&#xD;
    {&#xD;
        Integer request = this.InPort[1].RequestBuffer.removeFirst();&#xD;
        if (request != null)&#xD;
        {&#xD;
            sum = sum + request;&#xD;
        }&#xD;
    }&#xD;
    &#xD;
    returnValue = sum;&#xD;
}&#xD;
&#xD;
return returnValue;</body>
        </ownedBehavior>
