        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_TNfxwChiEe2Cds-4L53svA" name="RequestBarrierConnectorBehavior" specification="_YTdwpyhjEe2d6bRsEf3Ctg">
          <language>Alf</language>
          <body>Integer i = 1;

this.BarrierOn = false;

while (i &lt;= 3)
{
    Integer request = this.InPort[i].RequestBuffer;
    if (request == null)
    {
        this.BarrierOn = true;
    }
    i++;
}

Integer returnValue = this.DefaultReply;
if (this.BarrierOn == false)
{
    // Merge requests
    Integer mergedRequest = 0;
    i = 1;
    while (i &lt;= 3)
    {
        request = this.InPort[i].RequestBuffer;
        if (request != null)
        {
            mergedRequest += request;
             // Invalidate consumed request
            this.InPort[i].RequestBuffer = null;    
        }
        i++;
    }

    returnValue = this.OutPort[1].xOp(mergedRequest);
    this.BarrierOn = true;
}

return returnValue;</body>
        </ownedBehavior>
