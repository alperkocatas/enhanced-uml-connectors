        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2LVDEChhEe2Cds-4L53svA" name="MultiDestRequesterConnectorBehavior" specification="_Fr6cJShiEe2fgozkGCS6Tw">
          <language>Alf</language>
          <body>Integer i = 1; 
for (ifc in this.OutPort)  
{
    this.ReplyBuffer[i] = ifc.xOp(arg);
    i++; 
}

// Reply merge strategy:         
i = 1;
Integer sum = 0;
while (i&lt;=2)
{
    Integer intval = this.ReplyBuffer[i];
    
    if (intval != null)
    {
        sum = sum + intval;
         
        // Invalidate value in the single copy buffer
        this.ReplyBuffer[i] = null;        
    }
    i++;
}

return sum;
</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Fszz8ShiEe2fgozkGCS6Tw" name="Connector1_ConnectorClsBehavior" specification="_Fszz8ChiEe2fgozkGCS6Tw">
          <language>Alf</language>
          <body>Integer i = 1;
while (i &lt;= 2) 
{
    this.InPort[i] = new RequestPortCls(this);
    i = i + 1;
}
</body>
        </ownedBehavior>
