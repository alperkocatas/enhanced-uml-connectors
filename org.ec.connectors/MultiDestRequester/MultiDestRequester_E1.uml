      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2LVDEChhEe2Cds-4L53svA" name="MultiDestConnectorBehavior">
        <ownedAttribute xmi:type="uml:Property" xmi:id="_2zPNkAKDEe2EMb2arhAsgw" name="ReplyBuffer" visibility="package" isOrdered="true" isUnique="false" aggregation="composite"/>
        <language>Alf</language>
        <body>Integer i = 1; &#xD;
for (ifc in this.OutPort)  &#xD;
{&#xD;
    this.ReplyBuffer[i] = ifc.xOp(arg);&#xD;
    i++; &#xD;
}&#xD;
&#xD;
// Reply merge strategy:         &#xD;
i = 1;&#xD;
Integer sum = 0;&#xD;
while (i&lt;=PRV_CNT)&#xD;
{&#xD;
    Integer intval = this.ReplyBuffer[i];&#xD;
    &#xD;
    if (intval != null)&#xD;
    {&#xD;
        sum = sum + intval;&#xD;
         &#xD;
        // Invalidate value in the single copy buffer&#xD;
        this.ReplyBuffer[i] = null;        &#xD;
    }&#xD;
    i++;&#xD;
}&#xD;
&#xD;
return sum;&#xD;
</body>
