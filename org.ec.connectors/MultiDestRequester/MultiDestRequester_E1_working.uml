            <body>Integer i = 1; 
     	for (ifc in this.OutPort)  
     	{
     		this.ReplyBuffer[i] = ifc.xOp(arg);
     		i++; 
     	}
        
        // Reply merge strategy:     	
     	i = 1;
     	Integer sum = 0;
     	while (i&lt;=PRV_CNT)
     	{
     		Integer intval = this.ReplyBuffer[i];
     		
     		if (intval != null)
     		{
     			sum = sum + intval;
     		}
     		i++;
     	}
     	
        return sum;
</body>
