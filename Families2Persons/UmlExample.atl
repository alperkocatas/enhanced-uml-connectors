module UmlExample;

create OUT: UML2 refining IN: UML2;

rule Model {
	from s: UML2!"uml::Model" 
	to t:UML2!"uml::Model"()	
}

rule Class {
	from s: UML2!"uml::Class"
	to 
	t: UML2!"uml::Class" ( 
			name <- s.name
	), 
	t2: UML2!"uml::Property" ( 
			name <- s.name
	)
	
	-- how do we copy properties of the classes? 
	-- and refining also did not work. why? 
}
